{
  "name": "purified-image",
  "version": "0.2.0",
  "description": "Load, save and draw on image with API similar to HTML Canvas Context 2D. No native dependencies.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*.js' --ignore-pattern 'node_modules'",
    "test": "npm run lint && humile",
    "jsdoc": "jsdoc2md index.js | sed -e '1,27d;29d'> docs/api-Image.md"
  },
  "repository": "megahertz/node-purified-image",
  "files": [
    "index.js"
  ],
  "keywords": [
    "node",
    "image",
    "pureimage",
    "promise",
    "canvas",
    "jpg",
    "png",
    "draw",
    "font"
  ],
  "author": "Alexey Prokhorov",
  "license": "MIT",
  "bugs": "https://github.com/megahertz/node-purified-image/issues",
  "homepage": "https://github.com/megahertz/node-purified-image#readme",
  "dependencies": {
    "pureimage": "^0.1.6"
  },
  "engines": {
    "node": ">=8.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "humile": "^0.2.0",
    "jsdoc-to-markdown": "^5.0.0"
  }
}
